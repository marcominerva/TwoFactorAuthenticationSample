{
    "ConnectionStrings": {
        "SqlConnection": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=MinimalDB;Integrated Security=true"
    },
    "Authentication": {
        "DefaultScheme": "Bearer",
        "JwtBearer": {
            "SchemeName": "Bearer",
            "SecurityKey": "",
            "Issuers": [ "two-factor" ],
            "Audiences": [ "two-factor" ],
            "ExpirationTime": "01:00:00",
            "ClockSkew": "00:02:00",
            "EnableJwtBearerService": true
        }
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*"
}
